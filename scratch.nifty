// Labels can be used with break and continue.
// Can be useful if you have an inner and outer loop.

#outer!
for (i := 0; i < 100; ++i) {
    #inner!
    for (j := 100; j >= 0; --j) {
        if (someCondition) {
            continue #inner!
        }
        
        if (otherCondition) {
            break #outer!
        }
        
        // Stuff
    }
    
    // Stuff
}
